{
  "api": {
    "baseUrl": "http://capstore.fs.capgemini.com:2080/capserviceorm/userservices",
    "downloadUrl": "http://capstore.fs.capgemini.com:2080",
    "envUrl": "http://capstore.fs.capgemini.com:4000"
  },

  "paths": {
    "imagesRoot": "/assets/images/",
    "userImageFolder": "/assets/images/users/"
