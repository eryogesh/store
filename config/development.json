      "timeOut": 5000,
      "showProgressBar": true,
      "pauseOnHover": true,
      "position": ["top", "right"],
      "theClass": "sy-notification"
    },
    "unauthorizedEndpoints": ["api/products"],
    "notFoundEndpoints": ["api/products", "api/account/login", "api/account/register"]
  },
  "googleEnvAccountId":"UA-75976219-2",
  "debugging": true,
  "IsSSOEnabled": false
